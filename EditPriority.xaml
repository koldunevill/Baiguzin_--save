<Window x:Class="Baiguzin_Глазки_save.EditPriority"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Baiguzin_Глазки_save"
        mc:Ignorable="d"
        Title="Измение приоритета" Height="200" Width="300" FontFamily="Century Gothic">
    <Grid Margin="20">
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <TextBlock Text="Изменение приоритета" FontSize="20" HorizontalAlignment="Center"></TextBlock>
        <StackPanel Grid.Row="1" VerticalAlignment="Center" Orientation="Horizontal">
            <TextBlock Text="Приоритет:" FontSize="15"></TextBlock>
            <TextBox Name="Priority" Margin="10,0,0,0" Width="100"></TextBox>
        </StackPanel>
        <Button Name="SaveButton" Click="SaveButton_OnClick" Grid.Row="2" Content="сохранить"></Button>
    </Grid>
    
</Window>
